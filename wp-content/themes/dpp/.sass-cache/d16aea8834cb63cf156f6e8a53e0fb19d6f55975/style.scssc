:@template"23/*!
Theme Name: DPP
Theme URI:
Description: DPP Theme (Based off HTML-Reset theme).
Author: Numiko
Author URI: http://numiko.com
Version: 1
*/

/*
* 	Compass imports
*/
@import "compass/utilities";
@import "compass/css3";
@import "compass/typography";

/*
* 	Resets, Variables, Mixins
*/
@import "_variables.scss";
@import "_normalize.scss";
@import "_mixins.scss";

/*
* 	Custom Typography
*/
@import "_fontface.scss";

/*
* 	Base Elements Styles
*/
@import "_base.scss";

/*
* 	Helper classes
*/
@import "_helper.scss";

/*
* 	Grid  Structure
*/
@import "_grid.scss";

/*
* Clean List reuable Style
*/
.clean-list {
	margin:0;
	padding:0;

	> li {
		list-style: none;
	}
}



/*
* Primary header
*/
.primary-header {
	@extend .clearfix;
	background: $dppBlack;;

	.dpp-background {
		background: $dppBlack url('common/img/header-bg.jpg') no-repeat right top;
	}
	.logo {
		width:157px;
		height:105px;
		float:left;
		margin: 30px 0 36px;
		background: url("common/img/dpp-logo.jpg") no-repeat top left;
		text-indent: -9999px;

		a {
			display: block;
			width:100%;
			height:100%;
		}
	}
	.header-copy {
		text-align: right;
		padding:40px 0 0 0;

		h2 {
			color:#fff;
			margin-bottom:24px;
			font-family: #{$latoLightStack};
			font-size: pxtoem(16, $basepx);
		}
	}
}

/*
* Primary Nav
*/
.primary-nav {
	@extend .clearfix;
	background: #ededed;

	.menu {
		@include inline-block-list;

		li {

			a {
				font-family: LatoLight, Arial, sans-serif;
				font-size: pxtoem(18, $basepx);
				padding: pxtoem(20, 18) pxtoem(30, 18);
				display: block;
				text-decoration: none;
				color:$dppBody;
			}
			// Homepage link
			&.menu-item-home {
				a {
					width:85px;
					background: url("common/img/icons/icon-sprite.png") no-repeat center 19px;
					text-indent: -9999px;
				}
			}
			// active/hover menu item (show border)
			&.current-menu-item,
			&.current-page-ancestor {
				a {
					border-bottom:4px solid $dppHighlight;
					padding-bottom: pxtoem(16, 18);
				}

			}
		}
	}
}

/*
* Secondary Nav
*/
.secondary-nav {

	li {
		margin:0 0 10px 0;

		a {
			color:$dppBody;
		}
	}
	> li {
		@extend .chevron-before;
	}
	.children {
		display: none;
		margin-top:10px;
		padding-left:25px;

		li {
			list-style: none;
		}
	}
	.current_page_item,
	.current-cat {

		.children {
			display: block;
		}
		> a {
			font-weight:bold;
		}
	}
	.current_page_parent {
		.children {
			display: block;
		}
	}
}


/*
* Home Slider (Flexslider)
*/
@import "plugin-styles/flexslider.scss";
.news-slider {
	width:100%;
	height:227px;
	background: #eff6fc url(common/img/banner-bg.jpg) no-repeat top center;
	overflow:hidden;

	.slider-copy {
		width:433px;
		float:right;
		padding:30px 0 0 0;
		background: url(common/img/banner-bg-fade-fix.jpg) no-repeat top center;

		h2 {
			line-height:1.1em;
			font-size: pxtoem(27, $basepx);
		}
	}


}
.flexslider {
	background:transparent;
	border:0;
	margin:0;
	@include boxshadow("none");
	@include border-radius("0px");
	height:100%;

	.slides {
		height:100%;

		li {
			height:100%;

		}
	}

}
.flex-control-nav {
	position: absolute;
	bottom:0;
	width: $maxWidth;
	left:50%;
	margin-left:-($maxWidth/2);
	text-align:right;

	li {
		text-align: left;

		a {
			width:9px;
			height:9px;
			background:#d4d4d4;
			background-color:rgba(0,0,0,.3);

			&.flex-active {
				background-color: $dppHighlight;
			}
		}

	}
}


/*
* 2 per row List Style
*/
.two-per-row {
	margin-left:-20px;
	li {
		width: 50%;
		float:left;
		padding-left:20px;
	}
}

/*
* Content container styles
*/
.padded-content {
	padding:14px 20px 20px;
	overflow:hidden;
}
.dashed-bottom {
	border-bottom: 1px dashed #dddddd;
}
.dashed-top {
	border-top: 1px dashed #dddddd;
}
.highlight-bottom {
	border-bottom: 3px solid $dppHighlight;
}
.grey-header-box {
	border:1px solid #e7e7e7;
	@include border-radius(4px);
	background:#fff;

	h3 {
		background-color:#f0f0f0;
		margin:0;
		padding: 15px 20px;
	}
}

/*
* Download list
*/
.download-list {
	li {
		@extend .dashed-bottom;
		font-family: #{$latoRegularStack};
		a {
			padding:0px 20px;
			margin: 15px 0;
			display: inline-block;
			*display:inline;
			*zoom:1;
		}
	}
}
/*
* Primary  Content
*/
.primary-content {
	padding-top:30px;
}

/*
* Primary Footer
*/
.primary-footer {
	padding:30px 0;
	margin-top:30px;
	@include single-box-shadow(rgba(0,0,0,.02), 0, -3px, 1px, 0);

	p {
		color: $dppLightBody;
	}

	.menu {
		li {
			@extend .chevron-before;
			font-size: pxtoem(13, $basepx);
			margin-bottom:2px;

			a {
				color:$dppBody;
			}
		}
	}
}
.small-text {
	font-size: pxtoem(13, $basepx);
}
.footer-logos {
	text-align:right;

	li {
		display: inline-block;
		vertical-align: middle;
		margin:0 0 25px 45px;

		a {
			display: block;
		}
	}
}

.block-articles {
	@extend .clearfix;
	margin-bottom: pxtoem(20, $basepx);

	> li {
	 	margin-bottom:30px;
	}
	> li:last-child {
		margin-bottom:0;		// Remove margin on last item

		.for-members {
			border-bottom:0;	// Remove the border on last item of members list style
		}
	}
}

/*
* Block content Styles (inside <li>'s in 'the loop')
*/
.block-content {
	overflow: hidden;

	.meta-date {
		font-style:italic;
		font-size: pxtoem(13, $basepx);
	}

	&.outlined-box {
		background-color:#f3f3f3;
		border:1px solid #e7e7e7;
		@include border-radius(4px);
	}
	&.standard-block {
		min-height:362px;
	}

	// Side thumbnail version (shared)
	&.thumbnail-on-left  {

		.thumbnail-wrap {
			float:left;
		}
		.highlight-bottom {
			border:0;
		}
		.padded-content {
			padding-bottom:14px;
		}
		p {
			font-size:pxtoem(13, $basepx);
		}

		.more-link {
			font-size: pxtoem(13,$basepx);
		}
		.chevron-before {
			padding-left:15px;

			&:before {
				top:-4px;
			}
		}
		.meta-date {
			font-weight:bold;
		}

	}

	// Downloads version
	&.for-downloads {
		@extend .dashed-bottom;

		.thumbnail-wrap {
			width:140px;
			height:auto;
			padding-top:18px;
			margin-bottom:30px;

			img {
				display: block;
				margin:0 auto;
			}
		}
		.padded-content {
			padding-top:0;
			padding-bottom:30px;
		}
	}


	// Members version
	&.for-members {
		@extend .dashed-bottom;

		.thumbnail-wrap {
			background-color:#fff;
			padding:20px;
			border:1px solid #e9e9e9;
		}
		.member-position {
			font-style: italic;
			margin-bottom:1em;
		}
		.profile-default {
			width:96px;
		}
	}

}

/*
* Single page Styles
*/
.first-title {
	position: relative;
	margin-bottom: pxtoem(32, $basepx);
	h1, p {
		max-width: 80%;
	}
	p {
		margin-bottom:pxtoem(10, 30);
	}
}
.single-thumbnail-wrap {
	margin-bottom: pxtoem(15, $basepx);
}

/*
* Link Styles
*/
.chevron-before {
	padding-left:20px;
	position: relative;
	;
	&:hover {
		text-decoration: none;
	}
	span:hover {
		text-decoration: underline;
	}
	&:before {
		content: "\00BB"; // eqivilient of &raquo;
		color:$dppHighlight;
		font-size: pxtoem(21, $basepx);
		// margin-right:5px;
		position: absolute;
		left:0;
		top:-2px;
	}
}
a.chevron-before {
	color:$dppBody;
}
.more-downloads {
	text-align:right;
}

/*
* Btn Styles
*/
.btn,
.btn:link,
.btn:visited {
	@include inline-block;
	@include border-radius(4px);
	@include transition-property(background);
  	@include transition-duration(.2s);
  	@include transition-timing-function(ease-in);
	background-color: $dppHighlight;
	color:#fff;
	padding: pxtoem(10, 18) pxtoem(20, 18);
	font-family: #{$latoRegularStack};
	font-size: pxtoem(18, $basepx);
	text-decoration:none;
	margin-bottom: pxtoem(10, 18);

	&:hover {
		background-color: lighten($dppHighlight, 5%);
	}
}


/*
* Sidebar / Widget Styles
*/
.sidebar {
	padding-top:8px;

	h2 {
		padding-top:4px;
		margin-bottom: pxtoem(20, $sizeH3)
	}
	.chevron-before {
		float:right;
	}
	.widget {
		margin-bottom: 40px;
		@extend .clearfix;
	}
	.textwidget {
		@extend .clearfix;
	}
}
