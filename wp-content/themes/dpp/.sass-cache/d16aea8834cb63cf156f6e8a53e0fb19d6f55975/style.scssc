/* Twitter - styles for http://wordpress.org/plugins/display-tweets-php/ */
.widget_displaytweetswidget {

		h2 {
			background: url(common/img/icons/icon-sprite.png) no-repeat right -31px;
		}
		.tweet  {
			
			margin:0 0 40px 0;
			border: 1px solid #E7E7E7;
			background:#fff;
			border-radius: 4px;

			p {
				margin:0;
				padding:15px;
				border-bottom:1px dashed #E7E7E7;
			}
			small {
				display:block;
				text-align:right;
				position:relative;
				padding:10px 15px;

				&:after {
					content:"";
					width:21px;
					height:21px;
					background: url(common/img/misc-sprite.png) no-repeat 0 -43px;
					position: absolute;
					bottom:-21px;
					left:20px;

				}
			}
		}
		
}

/* Linked in widget */
.widget-linkedin {
	h2 {
		background: url(common/img/icons/icon-sprite.png) no-repeat right -72px;
	}
}
/* Share Btns */
.share-btns {
	position: absolute;
	top:15px;
	right:0;

	li {
		float:left;
	}
	li.twitter-share {
		width:95px;
	}
	li.twitter-follow {
		width:auto;
		margin-right:20px;
	}
	> iframe,
	> span {
		vertical-align: middle!important;
	}
}


/*
*	Skip to Nav
*/
.back-to-top {
	width:50px;
	height:47px;
	display:block;
	background: $dppHighlight url(common/img/misc-sprite.png) no-repeat 17px 10px;
	text-indent:-9999px;
	float:right;
	@include border-radius("4px 4px 0 0");


	&:active,
	&:focus {
		outline:0;
		-moz-outline-style: none;
	}
}

/*
* Entry WYSIWYG
*/
.entry {

	.edit-link {
		clear:both;
		padding-top:20px;

		a {
			font-weight: bold;
		}
	}
	img.alignright {
		float:right;
		margin:0 0 15px 15px;
	}
	img.alignleft {
		float:left;
		margin:0 15px 15px 0px;
	}
	blockquote {
		font-style: italic;
	}
	h2, h3, h4, h5 {
		margin-top:1.3em;

		&:first-child {
			margin-top:0;
		}
	}
	h3 {
		font-size: pxtoem(19, $basepx);
	}


} /* end .entry */

/*
* SS downloads (resetting hence needed some !importants)
*/
html #ss-downloads {
	border:0;
	background-color:transparent;
	padding:0;
	margin:0;

	h3 {
		display: none;
	}

	form .input-text {
		padding:  7px;
	}

	.btn-ss-downloads {
		background:none;
		height:auto;
		min-height: 0;
		padding:0;

		a {
			background:none;
		    color: #777 !important;
		    cursor: pointer;
		    display: block;
		    font-weight: bold !important;
		    height: auto;
		    line-height: normal;
		    margin: 0;
		    min-height: 0;
		    padding: 0;
		    text-decoration: none !important;

		    &:hover {
		    	color:inherit!important;
		    }
		}
	}
}


/*
* Form Styling
*/
form {
	@extend .clearfix;

	input[type="text"],
	input[type="email"],
	input[type="number"],
	input[type="password"],
	textarea {
		padding:7px;
		background:#fff;
		border:1px solid #777777;
		@include border-radius("4px");
	}

	input[type="submit"] {
		text-transform: capitalize;
		padding:7px 10px;
		background-color:#ededed;
		border:1px solid #777777;
		@include border-radius("4px");
	}

}
.error {
	color: $dppHighlight;
}
.form-item {
	margin-bottom: 1em;

	label,
	p.label {
		display: block;
		margin:0 0 0.5em 0;
		font-weight: bold;
	}
	.wpcf7-list-item {
		display: block;
		margin:0 0 5px 0;
	}
	.mailchimp-opt-in .wpcf7-list-item {
		@include inline-block;
	}
}
.form-right,
.form-left {
	width:45%;

	.form-item,
	input[type="text"],
	input[type="email"],
	input[type="number"],
	input[type="password"],
	textarea {
		width: 100%;
	}
}
.form-left{
	float:left;
}
.form-right {
	float:right;
}

/* Contact Form 7 overwrites */
.wpcf7-response-output {
	clear: both;
	float:left;
	font-weight:bold;
}
.wpcf7-validation-errors {
	display:none!important;
}
.wpcf7-mail-sent-ok {
	display:none!important;
}
body span.wpcf7-not-valid-tip {
	position: static;
	display:block!important;
	opacity:1!important;
	border:0;
	color: $dppHighlight;
	background:transparent;
}


/* === Print Styles === */
@import "_print.scss";


